{
  "name": "changeset-jira",
  "version": "0.1.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "changeset-jira": "dist/changeset-jira.mjs"
  },
  "scripts": {
    "dev": "tsdown --watch --no-dts",
    "build": "tsdown --clean",
    "changeset": "changeset",
    "changeset-jira": "tsdown --clean --no-dts && node dist/changeset-jira.mjs",
    "test": "vitest run",
    "release": "changeset publish",
    "version": "changeset version"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@types/node": "^25.2.0",
    "tsdown": "^0.20.1",
    "typescript": "~5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "meow": "^14.0.0"
  }
}
